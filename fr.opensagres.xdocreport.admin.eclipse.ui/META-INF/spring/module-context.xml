<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="repositoryExplorer" class="fr.opensagres.xdocreport.admin.eclipse.ui.views.RepositoryExplorer"
		scope="prototype">
		<property name="repositoryService" ref="repositoryService"></property>
	</bean>

	<bean id="repositoryEditor" class="fr.opensagres.xdocreport.admin.eclipse.ui.editors.repository.RepositoryEditor"
		scope="prototype">
		<property name="repositoryService" ref="repositoryService"></property>
	</bean>
	
	<bean id="folderResourceEditor" class="fr.opensagres.xdocreport.admin.eclipse.ui.editors.resources.FolderResourceEditor"
		scope="prototype">
		<property name="repositoryService" ref="repositoryService"></property>
	</bean>
	
	<bean id="fileResourceEditor" class="fr.opensagres.xdocreport.admin.eclipse.ui.editors.resources.FileResourceEditor"
		scope="prototype">
		<property name="repositoryService" ref="repositoryService"></property>
	</bean>
	  
</beans>